<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.Cursor?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ButtonBar?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<VBox alignment="CENTER" maxWidth="440.0" prefHeight="200.0" prefWidth="440.0" style="-fx-background-color: white;" xmlns="http://javafx.com/javafx/23.0.1" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <Label alignment="CENTER" contentDisplay="CENTER" prefHeight="53.0" prefWidth="445.0" style="-fx-background-color: transparent;" text="Confirm delete" textAlignment="CENTER">
         <font>
            <Font size="24.0" />
         </font>
         <VBox.margin>
            <Insets />
         </VBox.margin>
      </Label>
      <Label alignment="CENTER" contentDisplay="CENTER" prefHeight="53.0" prefWidth="436.0" style="-fx-background-color: transparent;" text="Are you sure you want to delete this record?" textAlignment="CENTER">
         <padding>
            <Insets left="20.0" top="10.0" />
         </padding>
         <font>
            <Font size="14.0" />
         </font>
      </Label>
      <ButtonBar maxWidth="200.0" prefHeight="65.0" prefWidth="195.0">
        <buttons>
          <Button contentDisplay="CENTER" mnemonicParsing="false" prefHeight="30.0" prefWidth="93.0" style="-fx-background-color: red; -fx-border-color: red;" text="Delete" textAlignment="CENTER" textFill="WHITE">
               <cursor>
                  <Cursor fx:constant="HAND" />
               </cursor>
            </Button>
            <Button minWidth="31.0" mnemonicParsing="false" prefHeight="30.0" prefWidth="92.0" style="-fx-background-color: black; -fx-border-color: black;" text="Go back" textFill="WHITE">
               <cursor>
                  <Cursor fx:constant="HAND" />
               </cursor>
            </Button>
        </buttons>
         <VBox.margin>
            <Insets top="-10.0" />
         </VBox.margin>
      </ButtonBar>
   </children>
</VBox>
