<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.Cursor?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ButtonBar?>
<?import javafx.scene.control.DatePicker?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.ColumnConstraints?>
<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.layout.RowConstraints?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<VBox alignment="CENTER" prefHeight="240.0" prefWidth="440.0" style="-fx-background-color: white;" xmlns="http://javafx.com/javafx/23.0.1" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <Label alignment="CENTER" contentDisplay="CENTER" prefHeight="53.0" prefWidth="436.0" style="-fx-background-color: transparent;" text="Update reservation" textAlignment="CENTER">
         <font>
            <Font size="24.0" />
         </font>
         <VBox.margin>
            <Insets />
         </VBox.margin>
         <padding>
            <Insets left="20.0" top="10.0" />
         </padding>
      </Label>
      <GridPane>
        <columnConstraints>
          <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
          <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
        </columnConstraints>
        <rowConstraints>
            <RowConstraints />
          <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
          <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
          <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
        </rowConstraints>
         <children>
            <Label text="Start time: " GridPane.rowIndex="2" />
            <DatePicker prefHeight="26.0" prefWidth="219.0" GridPane.columnIndex="1" GridPane.rowIndex="2" />
            <Label text="End time: " GridPane.rowIndex="3" />
            <DatePicker prefHeight="26.0" prefWidth="204.0" GridPane.columnIndex="1" GridPane.rowIndex="3" />
            <Label text="(Another field)" GridPane.rowIndex="4" />
            <TextField prefHeight="26.0" prefWidth="150.0" promptText="Another field" GridPane.columnIndex="1" GridPane.rowIndex="4">
               <cursor>
                  <Cursor fx:constant="TEXT" />
               </cursor></TextField>
            <Label text="Id:" GridPane.rowIndex="1" />
            <Label text="Id_value" GridPane.columnIndex="1" GridPane.rowIndex="1" />
         </children>
         <padding>
            <Insets bottom="10.0" left="20.0" right="20.0" top="20.0" />
         </padding>
      </GridPane>
      <ButtonBar prefHeight="65.0" prefWidth="399.0">
        <buttons>
          <Button mnemonicParsing="false" prefHeight="30.0" prefWidth="93.0" style="-fx-background-color: green; -fx-border-color: green;" text="Update" textFill="WHITE">
               <cursor>
                  <Cursor fx:constant="HAND" />
               </cursor>
            </Button>
            <Button minWidth="31.0" mnemonicParsing="false" prefHeight="30.0" prefWidth="92.0" style="-fx-background-color: black; -fx-border-color: black;" text="Go back" textFill="WHITE">
               <cursor>
                  <Cursor fx:constant="HAND" />
               </cursor>
            </Button>
        </buttons>
         <VBox.margin>
            <Insets right="20.0" />
         </VBox.margin>
      </ButtonBar>
   </children>
</VBox>
